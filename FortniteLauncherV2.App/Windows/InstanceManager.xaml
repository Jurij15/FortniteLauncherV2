<ui:UiWindow x:Class="FortniteLauncherV2.App.Windows.InstanceManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FortniteLauncherV2.App.Windows"
        mc:Ignorable="d"
             Background="Transparent"
             ResizeMode="CanMinimize"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        Title="Instance Manager" Height="358" Width="588">
    <Grid>
        <ListBox x:Name="BuildsList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,47,0,0"/>
        <ui:Button x:Name="StopButton" Click="StopButton_Click" Appearance="Danger" Content="Stop" Margin="374,10,0,0" VerticalAlignment="Top" Width="184"/>
        <CheckBox Content="GameServer" Margin="247,14,0,0" VerticalAlignment="Top" Width="122" Height="10"/>
        <ui:Button x:Name="CloseBtn" Click="CloseBtn_Click" Appearance="Secondary"  Content="Close" Margin="10,10,0,0" VerticalAlignment="Top" Width="147"/>

    </Grid>
</ui:UiWindow>
